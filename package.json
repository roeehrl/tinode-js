{
  "name": "@roeehrl/tinode-sdk",
  "description": "Tinode SDK fork with Storage interface for SQLite persistence in React Native",
  "version": "0.25.1-sqlite.6",
  "types": "./types/index.d.ts",
  "scripts": {
    "format": "js-beautify -r src/*.js",
    "build": "npm run format && npm run vers && npm run build:prod && npm run build:dev",
    "clean": "rm umd/*",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:docs": "jsdoc ./src -t ./node_modules/minami -d ../tinode.github.io/js-api",
    "vers": "echo \"export const PACKAGE_VERSION = \\\"`node -p -e \"require('./package.json').version\"`\\\";\" > version.js",
    "test": "jest"
  },
  "browserslist": ["defaults"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roeehrl/tinode-js.git"
  },
  "files": [
    "umd/tinode.dev.js",
    "umd/tinode.dev.js.map",
    "umd/tinode.prod.js",
    "umd/tinode.prod.js.map",
    "types/index.d.ts",
    "src/",
    "version.js"
  ],
  "keywords": [
    "instant messenger",
    "messenger",
    "chat",
    "sqlite",
    "react-native"
  ],
  "author": "Tinode Authors <info@tinode.co>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/roeehrl/tinode-js/issues"
  },
  "homepage": "https://github.com/roeehrl/tinode-js",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "react-native": "./src/index.native.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "react-native": "./src/index.native.js",
      "browser": "./umd/tinode.prod.js",
      "import": "./src/index.js",
      "require": "./umd/tinode.prod.js",
      "default": "./src/index.js"
    }
  },
  "peerDependencies": {
    "expo-sqlite": ">=14.0.0"
  },
  "peerDependenciesMeta": {
    "expo-sqlite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/plugin-proposal-numeric-separator": "^7.18.6",
    "@babel/preset-env": "^7.22.7",
    "babel-loader": "^10.0.0",
    "browserslist": "^4.21.9",
    "copy-webpack-plugin": "^13.0.0",
    "jest": "^30.0.0",
    "js-beautify": "^1.14.8",
    "jsdoc": "^4.0.2",
    "minami": "^1.2.3",
    "webpack": "^5.88.1",
    "webpack-cli": "^6.0.0"
  }
}
